{
  "name": "ktranslator",
  "description": "A Chrome Translator Extension",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["myStyles.css"],
      "js": ["contentScript.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/16.png",
      "24": "images/24.png",
      "32": "images/32.png"
    },
    "default_title": "ktranslator",
    "default_popup": "popup.html"
  }
}
